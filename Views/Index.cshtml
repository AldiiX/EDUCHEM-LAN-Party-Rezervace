@using EduchemLPR
<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/css/main.css?ver=@Program.ENV["CACHE_VERSION"]">
    <link rel="stylesheet" href="/css/home.css?ver=@Program.ENV["CACHE_VERSION"]">
    <link rel="stylesheet" href="/lib/pure-min.css">
    <link rel="stylesheet" href="/css/themes/light.css">
    <title>EDUCHEM LAN Party</title>

    <!-- Favicony -->
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    <link rel="icon" href="/favicon_black.ico" type="image/x-icon" media="(prefers-color-scheme: light)">
    <link rel="icon" href="/favicon_white.ico" type="image/x-icon" media="(prefers-color-scheme: dark)">

    @* OG *@
    <meta property="og:title" content="EDUCHEM LAN Party">
    <meta property="og:description" content="Web na rezervaci míst na LAN Party">
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://lanparty.educhem.it">
    <meta property="og:image" content="/images/logo/alt_w_b.png">
    <meta content="#FFB303" data-react-helmet="true" name="theme-color" />

    @* Twitter Card *@
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="EDUCHEM LAN Party">
    <meta name="twitter:description" content="Web na rezervaci míst na LAN Party">
    <meta name="twitter:image" content="/images/logo/alt_w_b.png">
</head>
<body>
    <div class="center">
        <h1>XMAS LAN Party - 2024</h1>

        <div class="menu">
            <a href="/rezervace" class="pure-button">Rezervace</a>
